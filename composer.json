{
  "name": "croustibat/Slack-Events",
  "description": "Slack Event WEbhook for Laravel",
  "keywords": [
    "slack",
    "laravel",
    "api",
    "slack api",
    "slack events api",
    "events",
    "slack events"
  ],
  "require": {
    "illuminate/http": "^8.0||^9.0",
    "illuminate/support": "^8.0||^9.0",
    "illuminate/routing": "^8.0||^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7||^5.0||^8.0||^9.0",
    "orchestra/testbench": "~3.0||^4.0||^5.1"
  },
  "autoload": {
    "psr-4": {
      "Lisennk\\LaravelSlackEvents\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lisennk\\LaravelSlackEvents\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lisennk\\LaravelSlackEvents\\SlackEventsServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Lisennk",
      "email": "serewann@gmail.com"
    }
  ]
}
